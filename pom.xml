        </mailingList>
        <mailingList>
            <name>Myfaces Developer List</name>
            <subscribe>dev-subscribe@myfaces.apache.org</subscribe>
            <unsubscribe>dev-unsubscribe@myfaces.apache.org</unsubscribe>
